@model IEnumerable<SQMS.Models.ViewModels.VMDashboardBranchService>
@using SQMS.Models.ViewModels

<div class="row">
    <div class="col-lg-12 scrollbar table-scroll">
        <b class="btn btn-color">Token Status:</b>
        <table class="table table-hover table-bordered" id="data">
            <thead>
                <tr id="Hedding">
                    <th>Service</th>
                    <th>Total</th>
                    <th>Served</th>
                    <th>Serving</th>
                    <th>Waiting</th>
                    <th>Missing</th>
                </tr>
            </thead>
            <tbody>
                @foreach (VMDashboardBranchToken item in ViewBag.tokenList)
                {
                    <tr>
                        @if (item.service_name.ToUpper() == "TOTAL")
                        {
                            <td>@item.service_name</td>
                            <td>@item.total</td>
                            <td>@item.served</td>
                            <td>@item.serving</td>
                            <td>@item.waiting</td>
                            <td>@item.missing</td>
                        }
                        else
                        {
                            <td>@item.service_name</td>
                            <td>@item.total</td>
                            <td>@item.served</td>
                            <td>@item.serving</td>
                            <td>@item.waiting</td>
                            <td>@item.missing</td>
                        }
                    </tr>
                }
            </tbody>
        </table>

        <b class="btn btn-color">Service Status:</b>
        <table class="table table-hover table-bordered" id="data">
            <thead>
                <tr id="Hedding">
                    <th>Service Name</th>
                    <th>Served</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        @if (item.service_name.ToUpper() == "TOTAL")
                        {
                            <td>@item.service_name</td>
                            <td>@item.served</td>
                        }
                        else
                        {
                            <td>@item.service_name</td>
                            <td>@item.served</td>
                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>

</div>



